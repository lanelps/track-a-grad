{
  "name": "track-a-grad",
  "version": "0.0.1",
  "description": "A way to sell our souls",
  "repository": "https://github.com/dev-academy-challenges/boilerplate-react-webpack.git",
  "main": "server/index.js",
  "author": "Track-A-Grad Team",
  "license": "ISC",
  "scripts": {
    "start": "concurrently \"node server\" \"webpack --watch\"",
    "dev": "concurrently \"yarn client\" \"yarn server\"",
    "client": "webpack --watch",
    "server": "nodemon server --ignore server/public",
    "knex": "knex",
    "test": "jest --watchAll",
    "updateDb": "rm -f dev.sqlite3 && yarn knex migrate:latest && yarn knex seed:run"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "bootstrap": "^4.3.1",
    "cheerio": "^1.0.0-rc.2",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.0",
    "jwt-decode": "^2.2.0",
    "knex": "^0.16.3",
    "libsodium-wrappers": "^0.7.4",
    "node": "8.15.0",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-hamburger-menu": "^1.1.1",
    "react-mail-form": "^1.0.2",
    "react-pose": "^4.0.8",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sqlite3": "^4.0.6",
    "superagent": "^4.1.0",
    "supertest": "^3.4.2",
    "webpack": "^4.41.4"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "style-loader": "^0.23.1",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
